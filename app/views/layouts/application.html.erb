<!DOCTYPE html>
<html>
<head>
  <title>Mycontact</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  
  <!-- navbar  -->
  <%= render 'navbar' %>
  
  <!-- content  -->
  <div class="container">
    <div class="row">
      
      <!-- sidebar  -->
      <%= render 'sidebar' %>
      
      <!-- Contacts  -->
      <%= yield %>
    </div>
  </div>



</body>
</html>
